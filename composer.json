{
  "name": "gbtux/php-watermark",
  "description": "Add text or image watermark on images.",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "watermark",
    "image",
    "image-manipulation",
    "imagemagick-watermark",
    "imagemagick-wrapper"
  ],
  "homepage": "https://github.com/ajaxray/php-watermark",
  "type": "library",
  "authors": [
    {
      "name": "Anis Uddin Ahmad",
      "email": "anis.programmer@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "ext-fileinfo": "*"
  },
  "autoload": {
    "psr-4": {
      "Ajaxray\\PHPWatermark\\": "src/Ajaxray/PHPWatermark/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ajaxray\\PHPWatermark\\Tests\\": "tests/Ajaxray/PHPWatermark/",
      "Ajaxray\\TestUtils\\": "tests/Ajaxray/TestUtils/"
    }
  },
  "require-dev": {
    "satooshi/php-coveralls": "*",
    "phpunit/phpunit": "9.*"
  }
}
